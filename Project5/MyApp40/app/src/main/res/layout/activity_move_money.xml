<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_move_money"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.jagadish.myapp40.MoveMoney">

    <TextView
        android:text="Type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spinner2"
        android:layout_alignStart="@+id/spinner2"
        android:layout_marginTop="18dp"
        android:id="@+id/textView46"
        android:layout_alignEnd="@+id/spinner2"
        android:textSize="24sp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:layout_below="@+id/textView46"
        android:layout_alignStart="@+id/textView46"
        android:layout_marginTop="15dp"
        android:id="@+id/editText17" />

    <TextView
        android:text="Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editText17"
        android:layout_alignStart="@+id/editText17"
        android:layout_marginTop="13dp"
        android:id="@+id/textView47"
        android:textSize="24sp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:layout_below="@+id/textView47"
        android:layout_alignStart="@+id/textView47"
        android:layout_marginTop="9dp"
        android:id="@+id/editText18" />

    <TextView
        android:text="Amount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editText18"
        android:layout_alignStart="@+id/editText18"
        android:layout_marginTop="15dp"
        android:id="@+id/textView48"
        android:textSize="24sp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:layout_below="@+id/textView48"
        android:layout_alignEnd="@+id/editText18"
        android:id="@+id/editText30"
        android:inputType="numberDecimal" />

    <TextView
        android:text="Category"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editText30"
        android:layout_alignStart="@+id/editText30"
        android:layout_marginTop="9dp"
        android:id="@+id/textView49"
        android:textSize="24sp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:layout_marginTop="11dp"
        android:id="@+id/editText31"
        android:layout_below="@+id/textView49"
        android:layout_alignStart="@+id/textView49"
        android:inputType="number" />

    <Spinner
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:id="@+id/spinner2"
        android:layout_below="@+id/textView45"
        android:layout_alignStart="@+id/textView45" />

    <Button
        android:text="Move Money"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button19"
        android:layout_below="@+id/editText31"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="Move Money From Account"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:id="@+id/textView44"
        android:textSize="24sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Spinner
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/spinner"
        android:layout_below="@+id/textView44"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Move Money to Account"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:layout_marginTop="14dp"
        android:id="@+id/textView45"
        android:textSize="24sp"
        android:layout_below="@+id/spinner"
        android:layout_alignStart="@+id/textView44" />

</RelativeLayout>
